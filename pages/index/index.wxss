view{
  color:var(--themeColor);
}
.search_input{
  height: 90rpx;
  padding: 10rpx;
  background-color: var(--themeColor);
}
.search_input navigator{
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 15rpx;
  color: #666;
}
.search_swiper swiper{
  width: 100%;
  height: calc(100vw  * 1440 / 1920);
  padding-bottom: 25rpx;
  background-color: var(--themeColor);
}
.search_swiper swiper image{
  width: 100%;
}
.search_navigator{
  display: flex;
}
.search_navigator navigator{
  padding: 30rpx 10rpx 10rpx 10rpx;
  flex: 1;
  border-bottom: 25rpx solid var(--themeColor);
}
.search_navigator navigator image{
  width: 100%;
}
.floor_title image{
  margin-left: 10rpx;
  width: 100%;
  padding-bottom: 1rpx;
}
.floor_list{
  overflow: hidden;
  padding-top: 20rpx;
  background-color: var(--themeColor);
}
.floor_list navigator{
  float: left;
  width: 33.3%;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}
.floor_list navigator:nth-last-child(-n+4){
  height: 33.33vw*386/232/2;
    border-left: 10rpx solid var(--themeColor);
}
.floor_list navigator:nth-child(2),.floor_list navigator:nth-child(3){
  border-bottom: 10rpx solid var(--themeColor);
}
.floor_list navigator:nth-child(1){
  border-left: 10rpx solid var(--themeColor);
}
.floor_list navigator:nth-child(3),.floor_list navigator:nth-child(5){
  border-right: 10rpx solid var(--themeColor);
}