.in_image{
  width: 100%;
}
.in_content{
  border:10rpx solid var(--themeColor);
  height: 80rpx;
  width: 540rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 110rpx;
  margin-bottom: 10rpx;
  border-radius: 15rpx;
  font-size: 38rpx;
  font-weight: 800;
  color: var(--themeColor);
  position: relative;
}
.out .in_content button{
  position: absolute;
  width: 100%;
  height: 100%;
  opacity: 0;
}
